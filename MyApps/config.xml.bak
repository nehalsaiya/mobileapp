<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>My Apps Demo</name>
    <description>
        Online Plat form for All Employee
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        My Apps Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="www/img/logo.png" />
        <splash density="mdpi" src="www/img/logo.png" />
        <splash density="hdpi" src="www/img/logo.png" />
        <splash density="xhdpi" src="www/img/logo.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-plugin-push" spec="^2.1.3">
        <variable name="SENDERID" value="446897882895" />
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
</widget>
